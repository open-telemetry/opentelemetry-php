{
    "name": "open-telemetry/extension-propagator-cloudtrace",
    "description": "CloudTraceContext propagator extension for OpenTelemetry PHP.",
    "keywords": ["opentelemetry", "otel", "tracing", "apm", "extension", "propagator", "cloudtrace"],
    "type": "library",
    "support": {
        "issues": "https://github.com/open-telemetry/opentelemetry-php/issues",
        "source": "https://github.com/open-telemetry/opentelemetry-php",
        "docs": "https://opentelemetry.io/docs/php",
        "chat": "https://app.slack.com/client/T08PSQ7BQ/C01NFPCV44V"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "opentelemetry-php contributors",
            "homepage": "https://github.com/open-telemetry/opentelemetry-php/graphs/contributors"
        }
    ],
    "require": {
        "php": "^8.2",
        "open-telemetry/api": "^1.0|^2.0",
        "open-telemetry/context": "^1.0|^2.0",
        "tbachert/spi": "^1.0.1"
    },
    "autoload": {
        "psr-4": {
            "OpenTelemetry\\Extension\\Propagator\\CloudTrace\\": "."
        },
        "files": [
            "_register.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev",
            "dev-2.x": "2.x-dev"
        },
        "spi-config": {
            "autoload-files": true,
            "prune-autoload-files": true
        }
    }
}
