{
  "name": "open-telemetry/transport-grpc",
  "description": "gRPC transport for OpenTelemetry PHP.",
  "keywords": ["opentelemetry", "otel", "tracing", "contrib", "transport", "otlp", "grpc"],
  "type": "library",
  "support": {
    "issues": "https://github.com/open-telemetry/opentelemetry-php/issues",
    "source": "https://github.com/open-telemetry/opentelemetry-php",
    "docs": "https://opentelemetry.io/docs/php",
    "chat": "https://app.slack.com/client/T08PSQ7BQ/C01NFPCV44V"
  },
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "opentelemetry-php contributors",
      "homepage": "https://github.com/open-telemetry/opentelemetry-php/graphs/contributors"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-grpc": "*",
    "grpc/grpc": "*",
    "open-telemetry/sdk": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "OpenTelemetry\\Contrib\\Grpc\\": "."
    },
    "files": [
      "_register.php"
    ]
  },
  "extra": {
    "spi": {
      "OpenTelemetry\\SDK\\Common\\Export\\TransportFactoryInterface": [
        "OpenTelemetry\\Contrib\\Grpc\\GrpcTransportFactory"
      ]
    },
    "spi-config": {
      "prune-autoload-files": [
        "src/Contrib/Grpc/_register.php"
      ]
    },
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  }
}
